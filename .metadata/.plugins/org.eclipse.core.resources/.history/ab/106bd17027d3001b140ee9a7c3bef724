<mapper namespace="kr.gsm.model.BoardMapper">
  <select id="boardList" resultType="kr.gsm.model.BoardVO">
      select * from tbl_board
  </select>
  <insert id="boardInsert">
      insert into tbl_boaard(title, contents, writer) 
      values(#(title), #(contents), #(writer))
  </insert>
</mapper>